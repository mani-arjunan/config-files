alt - 9 : \
window_id=$(yabai -m query --windows | jq '.[] | select(.app | ascii_downcase == "ghostty") | .id' | head -n1); \
if [ -n "$window_id" ]; then \
  yabai -m window --focus "$window_id"; \
else \
  open -na Ghostty; \
  sleep 0.5; \
  window_id=$(yabai -m query --windows | jq '.[] | select(.app | ascii_downcase == "ghostty") | .id' | head -n1); \
  [ -n "$window_id" ] && yabai -m window --manage off --layer below --move abs:100:100 --resize abs:1200:800; \
fi
