shift + alt - m : yabai -m window --toggle windowed-fullscreen

alt + shift - h : yabai -m window --toggle float; yabai -m window --grid 1:2:0:0:1:1  
alt + shift - l : yabai -m window --toggle float; yabai -m window --grid 1:2:1:0:1:1 
alt + shift - k : yabai -m window --toggle float; yabai -m window --grid 2:1:0:0:1:1  
alt + shift - j : yabai -m window --toggle float; yabai -m window --grid 2:1:0:1:1:1  

alt - 1 : \
window_id=$(yabai -m query --windows | jq '.[] | select(.app | ascii_downcase == "ghostty") | .id' | head -n1); \
[ -n "$window_id" ] && yabai -m window --focus "$window_id" || open -a Ghostty

